#!/system/bin/sh
clear
echo "RealHard ProJect"
echo
echo "Turning ON Default"
echo "waiting"
sleep 3
echo "Changing SELinux to Permissive"
sleep 1
setenforce 0
echo "SELinux Status is Permissive"
sleep 2
cmd power set-fixed-performance-mode-enabled true
sleep 1
clear
sleep1
echo schedutil > /sys/devices/system/cpu/cpufreq/policy0/scaling_governor
echo schedutil > /sys/devices/system/cpu/cpufreq/policy4/scaling_governor
echo schedutil > /sys/devices/system/cpu/cpufreq/policy7/scaling_governor
echo walt > /sys/devices/system/cpu/cpufreq/policy0/scaling_governor
echo walt > /sys/devices/system/cpu/cpufreq/policy4/scaling_governor
echo walt > /sys/devices/system/cpu/cpufreq/policy7/scaling_governor
clear
sleep 1
settings put system POWER_SAVE_MODE_OPEN 0
settings put system POWER_BALANCED_MODE_OPEN 1
settings put system power_mode high
echo "performance" > /sys/class/devfreq/1d84000.ufshc/governor
sleep 3
echo "- "
echo "   ▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒"
echo "   ▒ "
echo "   ▒▒▒▒ CODE NAME        : $(getprop ro.product.board) "
sleep 0.2
echo "   ▒▒▒▒ VERSION GL       : $(getprop ro.opengles.version) "
sleep 0.2
echo "   ▒▒▒▒ SELINUX          : $(getprop ro.boot.selinux) "
sleep 0.2
echo "   ▒▒▒▒ KERNEL           : $(uname -r) "
sleep 0.2
echo "   ▒▒▒▒ BUILD DATE       : $(getprop ro.system.build.date) "
slepp 0.2
echo "   ▒▒▒▒ ANDROID VERSION  : $(getprop ro.system.build.version.release_or_codename) "
sleep 0.2
echo "   ▒▒▒▒ ROM              : $(getprop ro.build.flavor) "
sleep 0.2
echo "   ▒▒▒▒ DESCRIPTION ROM  : $(getprop ro.build.description) "
sleep 0.2
echo "   ▒▒▒▒ FINGERPRINT      : $(getprop ro.build.fingerprint) "
sleep 0.2
echo "   ▒▒▒▒ SECURITY PATCH   : $(getprop ro.build.version.security_patch) "
echo "   ▒ "
echo "   ▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒▒"
echo "- "
sleep 0.2
echo "Changing CPU Governor to Default DONE"
clear
sleep 1
setenforce 1
echo "Changing SELinux to Enforce"
sleep 1
echo "DONE"

